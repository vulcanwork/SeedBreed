<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="SeedBreed.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:SeedBreed"
       xmlns:views="clr-namespace:SeedBreed.Views"
       Shell.FlyoutBehavior="{OnIdiom Phone=Flyout,Default=Locked}"
       Title="SeedBreed">

        <ShellContent Title="SeedBreed"
                      ContentTemplate="{DataTemplate local:MainPage}"
                      Route="MainPage" />

        <ShellContent Title="Seedling Source"
                      ContentTemplate="{DataTemplate views:SourceView}"
                      Route="SourceView" />


</Shell>
